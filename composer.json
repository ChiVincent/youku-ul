{
    "name": "chivincent/youku-ul",
    "description": "Youku uploader for Command Line Interface",
    "keywords": ["youku", "youku uploader", "console", "cli"],
    "homepage": "https://github.com/ChiVincent/youku-ul",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Chivincent/youku-ul/issues",
        "source": "https://github.com/Chivincent/youku-ul"
    },
    "authors": [
        {
            "name": "Vincent Chi",
            "email": "song374561@chivincent.net"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "chivincent/youku-uploader": "^1.2",
        "illuminate/database": "5.8.*",
        "illuminate/log": "5.8.*",
        "intonate/tinker-zero": "^1.0",
        "laravel-zero/framework": "5.8.*",
        "symfony/finder": "^4.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["youku-ul"]
}
